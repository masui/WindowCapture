{
    "title": "Fnキー単独でGyazo起動",
    "rules": [{
        "description": "Fnキー単独でGyazo起動",
        "manipulators": [{
            "type": "basic",
            "from": {
                "key_code": "fn"
            },
            "to_if_alone": [{
                "shell_command": "open /Applications/Gyazo.app;"
            }],
            "to_if_held_down": [{
                "shell_command": "/usr/bin/osascript /Users/masui/WindowCapture/windowcapture.as"
            }]
	}]
    }]
}
